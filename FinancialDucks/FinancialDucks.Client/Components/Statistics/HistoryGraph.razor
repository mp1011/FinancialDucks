<RadzenChart SeriesClick="SeriesClicked">
    <RadzenColumnSeries 
        Data="@TimeSlices" 
        CategoryProperty="Label"          
        Title="$" 
        LineType="LineType.Dashed" 
        ValueProperty="Value"  
        />
    
    <RadzenColumnOptions Radius="5" />
    <RadzenValueAxis Formatter="@Format">
        <RadzenGridLines Visible="true" />
    </RadzenValueAxis>
</RadzenChart>

